{
  "plugins": [
      "jest"
  ],
  "extends": [
      "plugin:jest/recommended"
  ],
  "parserOptions": {
      "ecmaVersion": 8
  },
  "globals": {
    "set": true,
    "context": true,
    "its": true,
    "subject": true,
    "isExpected": true
  },
  "rules": {
    "jest/no-identical-title": 0,
    "node/no-unpublished-require": 0,
    "promise/no-callback-in-promise": 0
  }
}
