{
  "name": "node-vcr",
  "version": "2.3.3",
  "description": "Record your test suite's HTTP interactions and replay them during future test runs for fast, deterministic, accurate tests",
  "main": "./lib/index.js",
  "scripts": {
    "build": "node_modules/.bin/babel src -d lib",
    "ci": "NODE_ENV=test node_modules/.bin/jest --watch",
    "deps": "yarn upgrade-interactive --latest",
    "jsdoc": "node_modules/.bin/jsdoc -c jsdoc.config.json",
    "lint": "node_modules/.bin/eslint .",
    "prepublish": "npm run build",
    "test": "NODE_ENV=test node_modules/.bin/jest",
    "test:server": "DEBUG=node-vcr* nodemon test-server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mbaertschi/node-vcr.git"
  },
  "keywords": [
    "nodejs",
    "npm",
    "vcr",
    "testing",
    "http"
  ],
  "author": "Michael BÃ¤rtschi <michael.baertschi@zebbra.ch> (http://zebbra.ch)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mbaertschi/node-vcr/issues"
  },
  "homepage": "https://github.com/mbaertschi/node-vcr#readme",
  "engines": {
    "node": ">=8.16.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "babel-preset-env": "^1.7.0",
    "docdash": "^1.1.1",
    "eslint": "^6.0.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jest": "^22.7.1",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^24.8.0",
    "jest-cli": "^24.8.0",
    "jest-junit": "^6.4.0",
    "jest-plugins": "^2.9.0",
    "jest-plugins-recommended": "^2.9.0",
    "jsdoc": "^3.6.2",
    "lodash": "^4.17.11",
    "nodemon": "^1.19.1",
    "npm-check": "^5.9.0",
    "pre-commit": "^1.2.2",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "content-type": "^1.0.4",
    "debug": "^4.1.1",
    "follow-redirects": "^1.7.0",
    "fs-extra": "^8.1.0",
    "incoming-message-hash": "^4.0.0",
    "json-bigint": "^0.3.0",
    "pretty-data": "^0.40.0"
  },
  "jest": {
    "rootDir": ".",
    "roots": [
      "<rootDir>/test/"
    ],
    "collectCoverage": true,
    "coverageDirectory": "reports/coverage",
    "reporters": [
      "default",
      "jest-junit"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/global.config.js"
    ]
  },
  "jest-junit": {
    "output": "reports/junit/js-test-results.xml"
  },
  "pre-commit": [
    "test",
    "lint"
  ]
}
